<?xml version='1.0' encoding='UTF-8'?>
<odoo>
     <data noupdate="1">
        <!--Secuencia para los movimientos en el detalle del pago-->
        <record id="sequence_approval_request" model="ir.sequence">
            <field name="name">Secuencia aprobación de solicitud</field>
            <field name="code">aliadas.approval.request</field>
            <field name="prefix">SOL-</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="6" name="padding"/>
            <field eval="False" name="company_id"/>
        </record>
    </data>

    <data noupdate="1">
        <!--Secuencia para los movimientos en el detalle del pago-->
        <record id="sequence_maintenance_master" model="ir.sequence">
            <field name="name">Secuencia Mantenimiento Master</field>
            <field name="code">aliadas.maintenance.master</field>
            <field name="prefix">MM-</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="6" name="padding"/>
            <field eval="False" name="company_id"/>
        </record>

        <record id="sequence_maintenance_periodic" model="ir.sequence">
            <field name="name">Secuencia Mantenimiento Periodico</field>
            <field name="code">aliadas.maintenance.periodic</field>
            <field name="prefix">MP-</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="6" name="padding"/>
            <field eval="False" name="company_id"/>
        </record>
    </data>

    <!--CONTRATO DE SUBSCRIPCIONES-->
    <data noupdate="1">
        <record id="sequence_subscription_contract_name" model="ir.sequence">
            <field name="name">Secuencia contrato</field>
            <field name="code">subscription.contract.name</field>
            <field name="prefix" />
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="13" name="padding"/>
            <field eval="False" name="company_id"/>
        </record>
    </data>

     <!--CHEQUES-->
    <data noupdate="1">
        <record id="sequence_payment_check" model="ir.sequence">
            <field name="name">Secuencia cheques</field>
            <field name="code">account.payment.check</field>
            <field name="prefix" />
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="9" name="padding"/>
            <field eval="False" name="company_id"/>
        </record>
    </data>

